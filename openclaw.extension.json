{
  "$schema": "https://openclaw.ai/schemas/extension.json",
  "id": "amped-openclaw",
  "name": "Amped OpenClaw",
  "version": "1.0.0",
  "description": "DeFi operations plugin for OpenClaw - cross-chain swaps, bridging, and money market via SODAX SDK",
  "author": "Amped Finance",
  "license": "MIT",
  "entry": "packages/amped-openclaw-plugin/dist/index.js",
  "openclaw": {
    "minVersion": "0.1.0",
    "plugins": [
      {
        "id": "amped-openclaw",
        "name": "Amped OpenClaw",
        "entry": "packages/amped-openclaw-plugin/dist/index.js",
        "skills": ["skills/amped-openclaw"]
      }
    ]
  },
  "keywords": [
    "defi",
    "sodax",
    "swap",
    "bridge",
    "money-market",
    "cross-chain",
    "lending",
    "borrowing"
  ],
  "categories": ["defi", "finance"],
  "repository": {
    "type": "git",
    "url": "https://github.com/amped-finance/amped-openclaw.git"
  },
  "bugs": {
    "url": "https://github.com/amped-finance/amped-openclaw/issues"
  },
  "homepage": "https://github.com/amped-finance/amped-openclaw#readme"
}
